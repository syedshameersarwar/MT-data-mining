@ARTICLE{Min2024-nc,
  title    = "Versatile human cardiac tissues engineered with perfusable heart
              extracellular microenvironment for biomedical applications",
  author   = "Min, Sungjin and Kim, Suran and Sim, Woo-Sup and Choi, Yi Sun and
              Joo, Hyebin and Park, Jae-Hyun and Lee, Su-Jin and Kim, Hyeok and
